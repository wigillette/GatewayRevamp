{
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
        "node_modules",
        "wrapper.js",
        "config.js"
    ],
    "coverageDirectory": "<rootDir>/coverage/",
    "coverageThreshold": {
        "global": {
            "branches": 20,
            "functions": 30,
            "lines": 50,
            "statements": 50
        }
    },
    "mapCoverage": true,
    "verbose": false
}